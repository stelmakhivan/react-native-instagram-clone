{
  "name": "react-native-instagram-clone",
  "version": "1.0.0",
  "description": "Instagram Clone â€” Express + React Native",
  "repository": "https://github.com/stelmakhivan/react-native-instagram-clone.git",
  "author": "Ivan Stelmakh <stelmakh.developer@gmail.com>",
  "license": "MIT",
  "scripts": {
    "server": "nodemon --watch server/ --exec babel-node server/server.js",
    "app": "yarn --cwd instaclone-app start",
    "client": "yarn --cwd client start",
    "serve": "concurrently \"yarn server\" \"yarn client\" \"yarn app\"",
    "serve:web": "concurrently \"yarn server\" \"yarn client\"",
    "serve:app": "concurrently \"yarn server\" \"yarn app\"",
    "generate": "prisma generate",
    "prisma": "yarn run deploy && yarn run generate",
    "deploy": "prisma deploy",
    "prebuild": "yarn run generate",
    "build": "babel server -d build",
    "postbuild": "cd server && copy ./api/**/*.graphql ../build/api/",
    "start": "node build/server.js"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/runtime": "^7.9.6",
    "@sendgrid/mail": "^6.5.4",
    "aws-sdk": "^2.668.0",
    "concurrently": "^5.1.0",
    "copy": "^0.3.2",
    "dotenv": "^8.2.0",
    "graphql-tools": "^4.0.7",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.6",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prisma": "^1.34.10",
    "prisma-client-lib": "^1.34.10"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.9.6",
    "nodemon": "^2.0.2"
  },
  "engines": {
    "node": "13.11.0",
    "npm": "6.13.7"
  }
}
